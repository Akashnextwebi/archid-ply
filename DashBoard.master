<%@ Master Language="C#" AutoEventWireup="true" CodeFile="DashBoard.master.cs" Inherits="DashBoard" %>

<!doctype html>
<html lang="en" data-bs-theme="light">
<meta http-equiv="content-type" content="text/html;charset=utf-8" />
